# Lombok configuration for aismessages project

# Add @Generated annotation to all Lombok-generated methods
# This helps with code coverage tools and Javadoc generation
lombok.addLombokGeneratedAnnotation = true

# Configure behavior for final fields
# Keeps fields final by default for immutability
lombok.fieldDefaults.defaultFinal = false

# Configure logging (if used in future)
# lombok.log.fieldName = logger

# Make sure generated equals/hashCode include callSuper by default for subclasses
# lombok.equalsAndHashCode.callSuper = call

# Flag any usage of experimental features
lombok.experimental.flagUsage = warning
